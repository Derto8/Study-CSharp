        </Grid>
        <ListBox Background="Wheat" Foreground="#FF553D3A" FontFamily="Helvetica" FontSize="16" x:Name="lb" Margin="10,123,110,154" Grid.RowSpan="3"/>
        <Label x:Name="label" Foreground="#FF553D3A" FontSize="16" FontFamily="Helvetica" Content="" HorizontalAlignment="Left" Margin="10,0,0,108" VerticalAlignment="Bottom" Grid.Row="2"/>
    </Grid>
</Page>
